{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "check:lint": "eslint **/*.{js,ts,vue} --ignore-path .gitignore",
    "fix:lint": "eslint **/*.{js,ts,vue} --ignore-path .gitignore --fix",
    "check:format": "prettier **/*.{js,ts,vue,json} '!package-lock.json' --ignore-path .gitignore --check",
    "fix:format": "prettier **/*.{js,ts,vue,json} '!package-lock.json' --ignore-path .gitignore --write",
    "check:type": "nuxt typecheck",
    "check": "npm-run-all check:*",
    "fix": "npm-run-all fix:*"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@types/node": "^18",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.5.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vue-tsc": "^1.6.5"
  }
}
